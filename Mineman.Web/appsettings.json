{
  "DockerOptions": {
    "DockerHost": "npipe://./pipe/docker_engine",
    "DockerfilePath": "../Mineman.Service/Resources/Dockerfile"
  },

  "PathOptions": {
    "WorldDirectory": "./bin/trash/worlds",
    "ServerPropertiesDirectory": "./bin/trash/properties",
    "ModDirectory": "./bin/trash/mods",
    "ImageZipFileDirectory": "./bin/trash/imagezipfiles"
  },

  "BackgroundServiceOptions": {
    "WorkingLoopSleepInterval": "0.00:02:00",
    "EnableBackgroundWorldProcessing": "False",
    "WorldProcessingInterval": "0.00:05:00"
  },

  "TextureOptions": {
    "BlockColorsFilePath": "../Mineman.WorldParsing/Resources/blockcolors.json",
    "BiomeColorsFilePath": "../Mineman.WorldParsing/Resources/biomecolors.json"
  },

  "ServerCommunicationOptions": {
    "QueryIpAddress": "10.0.75.1"
  },

  "ConnectionStrings": {
    "MainDatabase": "Filename=./database.db"
  },

  "RemoteImageOptions": {
    "Enable": "True",
    "RepositoryPath": "http://nyclon.crabdance.com/mineman-images/",
    "RefreshInterval": "0.01:00:00"
  },

    "Logging": {
      "IncludeScopes": false,
      "LogLevel": {
        "Default": "Debug",
        "System": "Information",
        "Microsoft": "Information",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    }
  }